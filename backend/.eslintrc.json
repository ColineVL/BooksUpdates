{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-use-before-define": "off",
        "indent": ["error", 4],
        "max-len": ["warn", {"code":  200, "comments":  200}]
    }
}
